{
  "name": "standard-hooks",
  "version": "0.0.0",
  "description": "Essential set of React Hooks for convenient Web API consumption.",
  "repository": "https://github.com/kripod/standard-hooks",
  "author": "Kristóf Poduszló <kripod@protonmail.com>",
  "license": "MIT",
  "scripts": {
    "doc": "documentation build src/*.ts --format md --github --parse-extension=ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,js,json,yml,md}\"",
    "lint": "eslint --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "dependencies": {
    "network-information-types": "^0.1.0"
  },
  "devDependencies": {
    "@pika/plugin-build-web": "^0.6.0",
    "@pika/plugin-ts-standard-pkg": "^0.6.0",
    "@testing-library/react": "^9.1.0",
    "@testing-library/react-hooks": "^2.0.1",
    "@types/jest": "^24.0.17",
    "@types/react": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "documentation": "^12.1.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.0",
    "eslint-plugin-jsdoc": "^15.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.9.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  }
}
